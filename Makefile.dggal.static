$(OBJ)fieldValue.sym: $(ECERE_SDK_SRC)/eda/libeda/src/fieldValue.ec
	$(ECP) $(CFLAGS) $(CECFLAGS) $(ECFLAGS) $(PRJ_CFLAGS) -c $(call quote_path,$(ECERE_SDK_SRC)/eda/libeda/src/fieldValue.ec) -o $(call quote_path,$@)

